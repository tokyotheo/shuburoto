<h1>New purchase</h1>

<% form_for(@purchase) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id, :readonly=>true %>
  </p>
  <p>
    <%= f.label :lecture_no %><br />
    <%= f.text_field :lecture_no, :readonly=>true %>
  </p>
  <p>
    <%= f.label :start %><br />
    <%= f.date_select :start %>
  </p>
  <p>
    <%= f.label :end %><br />
    <%= f.date_select :end, :include_blank => true, :default => nil %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', purchases_path %>
