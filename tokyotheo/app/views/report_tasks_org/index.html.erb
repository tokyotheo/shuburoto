<h1>Listing report_tasks</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Lecture</th>
    <th>Instructor</th>
    <th>Task</th>
    <th>Date</th>
  </tr>

<% for report_task in @report_tasks %>
  <tr>
    <td><%=h report_task.title %></td>
    <td><%=h report_task.lecture_id %></td>
    <td><%=h report_task.instructor_id %></td>
    <td><%=h report_task.task %></td>
    <td><%=h report_task.date %></td>
    <td><%= link_to 'Show', report_task %></td>
    <td><%= link_to 'Edit', edit_report_task_path(report_task) %></td>
    <td><%= link_to 'Destroy', report_task, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New report_task', new_report_task_path %>
